{
  "expo": {
    "name": "SacolApp",
    "slug": "SacolApp",
    "version": "0.0.5",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ],
      "package": "com.indivery.personal",
      "versionCode": 5
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permita $(PRODUCT_NAME) acessar sua localização.",
          "locationAlwaysPermission": "Permita $(PRODUCT_NAME) acessar sua localização.",
          "locationWhenInUsePermission": "Permita $(PRODUCT_NAME) acessar sua localização."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "2999b627-ab55-4b7b-ab9b-cc5e517a9867"
      }
    }
  }
}
